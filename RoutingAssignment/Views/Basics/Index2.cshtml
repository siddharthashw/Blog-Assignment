@model IEnumerable<RoutingAssignment.Models.Basic>

@{
    ViewBag.Title = "Index2";
}

<h2>Index</h2>

@Html.ActionLink("All", "Index")<br />
@Html.ActionLink("Date", "Index4")<br />
@Html.ActionLink("Grouped and Sorted LINQ", "Index3")<br />
@Html.ActionLink("Sorted and Group Lamdba", "Index5")
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model=>model.User.UserName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in ViewBag.contain)
    {
        <tr>
            <td>
                @item.Title.ToString()
            </td>
            <td>
                @item.Description.ToString()
            </td>
            <td>
                @item.Date.ToString()
            </td>
            <td>
                @item.User.UserName
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new {id = item.Id})
            </td>
        </tr>
    }
</table>